{
  "buildCommand": "cd frontend-web && npm ci --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend-web/dist",
  "installCommand": "cd frontend-web && npm ci --legacy-peer-deps --prefer-offline --no-audit",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
  